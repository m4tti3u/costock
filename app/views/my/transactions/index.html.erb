<div class="container-my-transaction">

  <div class="row pl-3 mw-100" >
      <div class="div side-navbar col-12 col-lg-4 " >
        <div class="div nft-category-card">
          <img class="avatar-large" alt="avatar-large" src="https://images-platform.99static.com/EI2mMgfeyCEQLwIUr2YEt2Sokcc=/1x0:1876x1875/500x500/top/smart/99designs-contests-attachments/96/96758/attachment_96758134" />
          <h3> Portfolio</h3>
        </div>

        <div class="div nft-category-card">
          <img class="avatar-large" alt="avatar-large" src="https://images-platform.99static.com/EI2mMgfeyCEQLwIUr2YEt2Sokcc=/1x0:1876x1875/500x500/top/smart/99designs-contests-attachments/96/96758/attachment_96758134" />
          <h4> Portfolio Cartes √† collectionerüÉè</h4>
        </div>

        <div class="div nft-category-card">
          <img class="avatar-large" alt="avatar-large" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR4BTCxK1UhtLFvEzlrdHs0cQocQGxQ-FuBoRO8HUalNz9od3L9kO57O9HtqXzhf8hDq9g&usqp=CAU" />
          <h4> Portfolio Sneakers üëü</h4>
        </div>

        <div class="div nft-category-card ">
          <img class="avatar-large" alt="avatar-large" src="https://i.pinimg.com/originals/e1/3b/9c/e13b9c6e166bbcd32d1392e7ad4a461b.jpg" />
          <h4> Portfolio Montres ‚åö</h4>
        </div>

        <a class="btn btn-transaction" href="#"> + Cr√©er un nouveau portfolio</a>
        <a class="btn btn-transaction" href="#"> + Mes asks</a>
        <a class="btn btn-transaction" href="#"> + Mes bids</a>
      </div>

      <div class="container col-12 col-lg-8">
        <div class="div card-category-current-balance text-center" >
          <h2 class='mt-3'> Valeur de mes avoirs NFT </h2>
          <%# <img class="image-current-balance mt-3 mb-5" src="https://image.freepik.com/vecteurs-libre/graphique-boursier-forex-concept-graphique_73426-35.jpg" /> %>
          <%= area_chart(vendredi: 12, samedi: 6, dimanche:5, lundi: 7, mardi: 2, mercredi: 6, jeudi: 10) %>
        </div>

      <div class="summary my-nfts text-center m-3">
        <%# @nfts.each do |nft| %>
          <p>Nombre de NFT: <%= @nfts.count %></p>
          <p>Valorisation totale: <%= @valorisation %>‚Ç¨</p>
        <%# end %>
      </div>

        <% @collectibles.each do |collectible| %>
          <div class="div nft-list-transaction px-3 mt-3">
            <div class="div">
              <p class="title-nft-list">Nom</p>
              <p class="nft-title"> <%= collectible.model %></p>
            </div>
            <div class="div">
              <%= cl_image_tag collectible.photo.key, class: "avatar-large"%>
            </div>
            <div class="div">
              <p class="title-nft-list">Cat√©gorie</p>
              <p><%= collectible.category %></p>
            </div>
            <div class="div">
              <p class="title-nft-list">Ref.</p>
              <p><%= collectible.reference %></p>
            </div>
            <div class="div">
              <p class="title-nft-list">Val. Resell</p>
              <p><%= collectible.resell_value %> ‚Ç¨</p>
            </div>
            <div class="div">
              <p class="title-nft-list">Nbre NFT</p>
              <p><%= collectible.nft_number %></p>
            </div>
            <div class="div">
              <p class="title-nft-list">Valo. </p>
              <% if collectible.bids.highest.nil? %>
                <p> -- ‚Ç¨</p>
              <% else %>
                <p><%= collectible.bids.highest.bid_price %> ‚Ç¨</p>
              <% end %>
            </div>

            <div class="div">
              <p class="title-nft-list">Valo.</p>
              <p><i class="fas fa-chart-line"></i> 18%</p>
            </div>
            <div class="div">
              <%= link_to 'Modifier', collectible_path(collectible)  %>
            </div>
          </div>
        <% end %>

      </div>

  </div>

</div>
